{
  "name": "@apparts/login",
  "version": "3.1.4",
  "description": "Login and Signup for Web, Apps, Server",
  "main": "dist/index.js",
  "module": "index.js",
  "scripts": {
    "test": "react-scripts test",
    "testOnce": "react-scripts test --watchAll=false",
    "testRun": "react-scripts start",
    "build": "rollup -c",
    "buildWatch": "rollup -c -w",
    "prepublishOnly": "npm run testOnce && npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@ph-uhl.com:Apparts/apparts-login.git"
  },
  "keywords": [
    "apparts",
    "web",
    "webapp",
    "app",
    "client",
    "node",
    "login",
    "signup"
  ],
  "author": "Philipp Uhl",
  "license": "UNLICENSED",
  "dependencies": {
    "@apparts/api": "^3.1.1",
    "@apparts/config": "^1.0.5",
    "formik": "^2.2.6",
    "prop-types": "^15.7.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@apparts/redux": "2.0.2",
    "@apparts/web-components": "^1.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.8.3",
    "eslint": "^7.21.0",
    "jsonwebtoken": "^8.5.1",
    "react-scripts": "^4.0.3",
    "web-vitals": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/src/tests",
      "/src/index.js",
      "/src/testApi.js",
      "/src/redux/testStore.js"
    ],
    "watchPathIgnorePatterns": [
      "/\\.#.*",
      "/.*~",
      "/#.*#"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
